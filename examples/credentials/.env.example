# .env.example - Template for environment variables
#
# Copy this file to .env and fill in your values
# This file CAN be committed to git as it contains no secrets

# Database configuration
DB_PATH=./data
DB_FORMAT=json

# For production databases, you would have:
# DB_HOST=
# DB_PORT=
# DB_NAME=
# DB_USER=
# DB_PASSWORD=
# DB_SSL_MODE=

# Redis cache
# REDIS_URL=
# REDIS_PASSWORD=

# Cloud credentials
# AWS_REGION=
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=

# API keys
# API_KEY=
# SMTP_PASSWORD=
